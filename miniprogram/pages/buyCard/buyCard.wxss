/* pages/buyCard/buyCard.wxss */
.container {
  width: 100%;
  height: 2000rpx;
  position: relative;
  background-image: url('http://view.jqueryfuns.com/2019/10/9/a9a16f16bb0b492de56a0fc1b6346f8d/images/login-bg.jpg');
}
.image {
  width: 96%;
  position: absolute;
  top: 6%;
}
.image .com {
  height: 520rpx;
  position: relative;
}
.image text {
  position: absolute;
  right: 0;
  bottom: 0;
}
.image image {
  width: 100%;
  height: 480rpx;
  width: 100%;
  border-radius: 10rpx;
  overflow: hidden;
}
.week {
  margin-top: 20rpx;
}