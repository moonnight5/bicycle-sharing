/* pages/set/set.wxss */
.container {
  width: 100%;
  height: 2000rpx;
  background-color:#f0f3f6;
}
.box {
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  margin-top: 10rpx;
  background-color: #ffffff;
  position: relative;
}
.special {
  position: fixed;
  bottom: 3%;
  color: red;
  display: block;
  text-align: center;
  line-height: 100rpx;
}
.box .title {
  align-self: center;
  margin-left: 2%;
  font-size: 36rpx;
}
.box .icon image{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  right: 2%;
}
.toast {
  width: 250rpx;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  font-size: 30rpx;
  border-radius: 26rpx;
  overflow: hidden;
  color: #ffffff;
  background-color: #74C3CA;
}